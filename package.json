{
  "name": "canon-tech",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.13.1"
  },
  "scripts": {
    "server": "nodemon --exec babel-node server/app.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build-server": "npx babel server --out-dir build",
    "start": "node build/app.js",
    "heroku-postbuild": "if [ -f \"build/images\" ]; then cp build/images . ; fi && rm -rf build && rm -rf client/build && npm install && npm run build-server && if [ -f \"./images\" ]; then cp ./images build ; fi && mkdir build/images && cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ele-cren/canon-tech.git"
  },
  "keywords": [],
  "author": "",
